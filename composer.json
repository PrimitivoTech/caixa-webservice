{
    "name": "primitivo/caixa-webservice",
    "description": "Package to emit brazilian boletos on Caixa bank",
    "type": "libray",
    "license": "MIT",
    "authors": [
        {
            "name": "Matheus Lopes Santos",
            "email": "fale_com_lopez@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Primitivo\\Caixa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "laravel/pint": "^1.8",
        "pestphp/pest": "^2.5"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "fix": [
            "./vendor/bin/pint"
        ],
        "verify": [
            "./vendor/bin/pint --test",
            "./vendor/bin/phpstan analyse"
        ]
    }
}
